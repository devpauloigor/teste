<p id="notice"><%= notice %></p>

<h1>Listing Auditas</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Questao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @auditas.each do |audita| %>
      <tr>
        <td><%= audita.user_id %></td>
        <td><%= audita.questao_id %></td>
        <td><%= link_to 'Show', audita %></td>
        <td><%= link_to 'Edit', edit_audita_path(audita) %></td>
        <td><%= link_to 'Destroy', audita, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Audita', new_audita_path %>
